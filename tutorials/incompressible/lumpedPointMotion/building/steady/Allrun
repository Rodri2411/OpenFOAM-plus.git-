#!/bin/sh
cd ${0%/*} || exit 1    # Run from this directory
. $WM_PROJECT_DIR/bin/tools/RunFunctions

./Allrun.pre

unset parallel
parallel=true

if [ "${parallel:-false}" = false ]
then
    # Serial

    runApplication simpleFoam

else
    # Parallel

    runParallel simpleFoam

fi


#------------------------------------------------------------------------------
