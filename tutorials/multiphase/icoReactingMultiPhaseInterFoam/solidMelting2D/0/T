FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 301.3;

boundaryField
{
    left
    {
        type           fixedValue;
        value          uniform 311;
    }
    right
    {
        type           inletOutlet;
        inletValue     $internalField;
        value          $internalField;

    }
    bottom
    {
        type            zeroGradient;
        value           $internalField;
    }
    top
    {
        type            zeroGradient;
        value           $internalField;
    }
    frontAndBack
    {
        type           empty;
    }
}


// ************************************************************************* //
