// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

blockage
{
    type            multiphasePorositySource;
    active          yes;

    multiphasePorositySourceCoeffs
    {
        porosity        porosity;

        regions
        {
            region1
            {
                cellZone        c0;
                a               0;
                b               2;
                c               0.34;
                porosity        0.49;
                D50             0.0159;
            }
        }
    }
}


